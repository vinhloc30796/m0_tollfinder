{
	"name": "m0_tollfinder",
	"description": "M0 Bridge Fee Estimator â€” Query cross-chain transfer costs (ethers v6)",
	"private": true,
	"scripts": {
		"build": "tsc -b && vite build",
		"cf-typegen": "wrangler types",
		"check": "tsc && CLOUDFLARE_ENV=production vite build && wrangler deploy --dry-run",
		"deploy": "CLOUDFLARE_ENV=production vite build && wrangler deploy",
		"dev": "vite",
		"lint": "eslint .",
		"preview": "npm run build && vite preview",
		"cli": "tsx src/cli.ts",
		"d1:migrate": "wrangler d1 migrations apply hypervitals_db",
		"d1:migrate:local": "wrangler d1 migrations apply hypervitals_db --local",
		"d1:migrate:prod": "wrangler d1 migrations apply hypervitals_db --remote",
		"d1:exec": "wrangler d1 execute DB --local",
		"d1:stdin": "sqlite3 \"$(find .wrangler/state/v3/d1/miniflare-D1DatabaseObject -name '*.sqlite' -type f -printf '%T@ %p\n' | sort -nr | head -n1 | cut -d' ' -f2-)\"",
		"cron:trigger": "bash -c 'while true; do now=$(date +%s); next=$((now - now % 60 + 60)); sleep $((next - now)); curl -s \"http://localhost:5173/cdn-cgi/handler/scheduled?cron=*+*+*+*+*\"; echo; done'",
		"cron:cleanup": "curl -s \"http://localhost:5173/cdn-cgi/handler/scheduled?cron=0+0+*+*+*\"; echo"
	},
	"cloudflare": {
		"label": "m0_tollfinder",
		"products": [
			"Workers"
		],
		"categories": [],
		"preview_icon_url": "https://imagedelivery.net/wSMYJvS3Xw-n339CbDyDIA/532a5378-9717-4964-385b-5fafd2160200/public",
